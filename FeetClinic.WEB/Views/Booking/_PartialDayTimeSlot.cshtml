@using BE.BE.Schedule
@model FeetClinic.WEB.Models.DayTimeSlotViewModel

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date.DayOfWeek.ToString())
        </th>
        <th></th>
    </tr>

@foreach (var timeSlot in Model.TimeSlots) {
    <tr>
        <td>
            @Html.Display(timeSlot.StartTime.ToString());
        </td>
    </tr>
}

</table>
